<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	android:versionCode="201108120"
	android:versionName="0.1.0" package="ch.unibas.urz.android.vv">
	<uses-sdk android:minSdkVersion="7" />
	<uses-permission android:name="android.permission.INTERNET"></uses-permission>

	<application android:label="@string/app_name" android:icon="@drawable/vvonline">
	<activity android:name=".view.activity.VvMainActivity"
		android:label="@string/app_name">
		<intent-filter>
			<action android:name="android.intent.action.MAIN" />
			<category android:name="android.intent.category.LAUNCHER" />
		</intent-filter>
	<intent-filter>
		<action android:name="android.intent.action.VIEW"></action>
		<category android:name="android.intent.category.DEFAULT"></category>
		<data android:mimeType="vnd.android.cursor.item/ch.unibas.urz.android.vorlesungsverzeichnis.entity"></data>
	</intent-filter>
	</activity>
		<provider android:name=".provider.VvContentProvider" android:authorities="ch.unibas.urz.android.vorlesungsverzeichnis"></provider>

	</application>
</manifest>